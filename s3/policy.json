{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListBucket",
      "Effect": "Allow",
      "Principal": {
        "AWS": "/// arn of the another account that you want to give access ///"
      },
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::jenish-chauhan-2005/*"
    },
    {
      "Sid": "AllowGetObjects",
      "Effect": "Allow",
      "Principal": {
        "AWS": "/// arn of the another account that you want to give access ///"
      },
      "Action": ["s3:GetObject", "s3:GetObjectVersion"],
      "Resource": "arn:aws:s3:::jenish-chauhan-2005/*"
    }
  ]
}
